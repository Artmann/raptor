{
  "name": "raptor",
  "version": "0.1.0",
  "description": "An embedding database that stores text indexes",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "raptor": "./dist/cli.js"
  },
  "scripts": {
    "build": "rolldown --config rolldown.config.ts && tsc --emitDeclarationOnly --declaration --outDir dist",
    "cli": "bun src/cli.ts",
    "dev": "bun run src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "lint:check": "eslint src/ --ext .ts,.tsx --max-warnings 0",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cleye": "^1.3.2",
    "fastembed": "^2.0.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.6.2",
    "rolldown": "^1.0.0-beta.44",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
